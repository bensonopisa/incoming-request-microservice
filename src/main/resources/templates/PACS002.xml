<?xml version="1.0" encoding="utf-8"?>
<hdr:Message xmlns:hdr="urn:montran:message.01">
    <hdr:AppHdr xmlns="urn:iso:std:iso:20022:tech:xsd:head.001.001.01">
        <Fr>
            <FIId>
                <FinInstnId>
                    <BICFI>{{fr_bicfi}}</BICFI>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{fr_memberId}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </Fr>
        <To>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{member_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </To>
        <BizMsgIdr>{{messageId}}</BizMsgIdr>
        <MsgDefIdr>pacs.002.001.07</MsgDefIdr>
        <BizSvc>RTP</BizSvc>
        <CreDt>{{create_date}}</CreDt>
    </hdr:AppHdr>
    <hdr:FIToFIPmtStsRpt xmlns="urn:iso:std:iso:20022:tech:xsd:pacs.002.001.07">
        <GrpHdr>
            <MsgId>{{messageId}}</MsgId>
            <CreDtTm>{{creationDateTime}}</CreDtTm>
            <InstgAgt>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{fr_memberId}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </InstgAgt>
        </GrpHdr>
        <OrgnlGrpInfAndSts>
            <OrgnlMsgId>{{original_messageId}}</OrgnlMsgId>
            <OrgnlMsgNmId>pacs.008.001.07</OrgnlMsgNmId>
            <GrpSts>{{grp_sts}}</GrpSts>
        </OrgnlGrpInfAndSts>
        <TxInfAndSts>
            <StsId>{{messageId}}</StsId>
            <OrgnlInstrId>{{original_instrumentId}}</OrgnlInstrId>
            <OrgnlEndToEndId>{{original_endToEndId}}</OrgnlEndToEndId>
            <OrgnlTxId>{{original_transactionId}}</OrgnlTxId>
            <AccptncDtTm>{{acceptance_dateTime}}</AccptncDtTm>
            <OrgnlTxRef>
                <PmtTpInf>
                    <SvcLvl>
                        <Prtry>INST</Prtry>
                    </SvcLvl>
                    <LclInstrm>
                        <Cd>{{local_instrument}}</Cd>
                    </LclInstrm>
                    <CtgyPurp>
                        <Cd>{{ctgy_purp}}</Cd>
                    </CtgyPurp>
                </PmtTpInf>
                <DbtrAgt>
                    <FinInstnId>
                        <BICFI>{{dbtr_bicFi}}</BICFI>
                        <ClrSysMmbId>
                            <ClrSysId>
                                <Prtry>PAPSS</Prtry>
                            </ClrSysId>
                            <MmbId>{{dbtr_memberId}}</MmbId>
                        </ClrSysMmbId>
                    </FinInstnId>
                </DbtrAgt>
            </OrgnlTxRef>
        </TxInfAndSts>
    </hdr:FIToFIPmtStsRpt>
</hdr:Message>