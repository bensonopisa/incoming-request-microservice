<?xml version="1.0" encoding="utf-8"?>
<hdr:Message xmlns:hdr="urn:montran:message.01">
    <hdr:AppHdr xmlns="urn:iso:std:iso:20022:tech:xsd:head.001.001.01">
        <Fr>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{senderPapssId}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </Fr>
        <To>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{toClearingSystemId}}</MmbId> <!-- confirm value for this field-->
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </To>
        <BizMsgIdr>{{messageId}}</BizMsgIdr>
        <MsgDefIdr>acmt.023.001.02</MsgDefIdr>
        <BizSvc>RTP</BizSvc>
        <CreDt>{{creationDateTime}}</CreDt>
    </hdr:AppHdr>
    <hdr:IdVrfctnReq xmlns="urn:iso:std:iso:20022:tech:xsd:acmt.023.001.02">
        <Assgnmt>
            <MsgId>{{messageId}}</MsgId>
            <CreDtTm>{{creationDateTime}}</CreDtTm>
            <Assgnr>
                <Agt>
                    <FinInstnId>
                        <BICFI>{{senderBicCode}}</BICFI>
                        <ClrSysMmbId>
                            <ClrSysId>
                                <Prtry>PAPSS</Prtry>
                            </ClrSysId>
                            <MmbId>{{senderPapssId}}</MmbId>
                        </ClrSysMmbId>
                    </FinInstnId>
                </Agt>
            </Assgnr>
            <Assgne>
                <Agt>
                    <FinInstnId>
                        <ClrSysMmbId>
                            <ClrSysId>
                                <Prtry>PAPSS</Prtry>
                            </ClrSysId>
                            <MmbId>{{receiverPapssId}}</MmbId>
                        </ClrSysMmbId>
                    </FinInstnId>
                </Agt>
            </Assgne>
        </Assgnmt>
        <Vrfctn>
            <Id>{{messageId}}</Id>
            <PtyAndAcctId>
                <Acct>
                    <Othr>
                        <Id>{{accountNumber}}</Id>
                        <SchmeNm>
                            <Cd>BBAN</Cd>
                        </SchmeNm>
                    </Othr>
                </Acct>
            </PtyAndAcctId>
        </Vrfctn>
    </hdr:IdVrfctnReq>
</hdr:Message>
