<env:Message xmlns:env="urn:montran:message.01">
    <env:AppHdr xmlns="urn:iso:std:iso:20022:tech:xsd:head.001.001.01">
        <Fr>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{receiver_papss_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </Fr>
        <To>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{toClearingSystemId}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </To>
        <BizMsgIdr>{{messageId}}</BizMsgIdr>
        <MsgDefIdr>pacs.004.001.07</MsgDefIdr>
        <BizSvc>RTP</BizSvc>
        <CreDt>{{creationDateTime}}</CreDt>
    </env:AppHdr>
    <env:PmtRtr xmlns="urn:iso:std:iso:20022:tech:xsd:pacs.004.001.07">
        <GrpHdr>
            <MsgId>{{messageId}}</MsgId>
            <CreDtTm>{{creationDateTime}}</CreDtTm>
            <NbOfTxs>1</NbOfTxs>
            <TtlRtrdIntrBkSttlmAmt Ccy="{{receiver_currency}}">{{receiver_amount}}</TtlRtrdIntrBkSttlmAmt>
            <IntrBkSttlmDt>{{interbankSttlmtDate}}</IntrBkSttlmDt>
            <SttlmInf>
                <SttlmMtd>CLRG</SttlmMtd>
                <ClrSys>
                    <Prtry>PAPSS</Prtry>
                </ClrSys>
            </SttlmInf>
            <InstgAgt>
                <FinInstnId>
                    <BICFI>{{receiver_bic_id}}</BICFI>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{receiver_papss_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </InstgAgt>
        </GrpHdr>
        <TxInf>
            <RtrId>{{messageId}}</RtrId>
            <OrgnlGrpInf>
                <OrgnlMsgId>{{original_messageId}}</OrgnlMsgId>
                <OrgnlMsgNmId>pacs.008.001.07</OrgnlMsgNmId>
            </OrgnlGrpInf>
            <OrgnlInstrId>{{original_messageId}}</OrgnlInstrId>
            <OrgnlEndToEndId>{{original_end_to_end_ref}}</OrgnlEndToEndId>
            <OrgnlTxId>{{original_messageId}}</OrgnlTxId>
            <OrgnlIntrBkSttlmAmt Ccy="{{sender_currency}}">{{sender_amount}}</OrgnlIntrBkSttlmAmt>
            <RtrdIntrBkSttlmAmt Ccy="{{receiver_currency}}">{{receiver_amount}}</RtrdIntrBkSttlmAmt>
            <ChrgBr>SLEV</ChrgBr>
            <RtrRsnInf>
                <Orgtr>
                    <Id>
                        <OrgId>
                            <AnyBIC>{{receiver_bic_id}}</AnyBIC>
                            <Othr>
                                <Id>{{receiver_papss_id}}</Id>
                            </Othr>
                        </OrgId>
                    </Id>
                </Orgtr>
                <Rsn>
                    <Cd>{{return_reason_code}}</Cd>
                </Rsn>
            </RtrRsnInf>
            <OrgnlTxRef>
                <IntrBkSttlmDt>{{creationDateSimple}}</IntrBkSttlmDt>
                <SttlmInf>
                    <SttlmMtd>CLRG</SttlmMtd>
                    <ClrSys>
                        <Prtry>PAPSS</Prtry>
                    </ClrSys>
                </SttlmInf>
                <PmtTpInf>
                    <SvcLvl>
                        <Prtry>INST</Prtry>
                    </SvcLvl>
                    <LclInstrm>
                        <Cd>{{instrm_type}}</Cd>
                    </LclInstrm>
                    <CtgyPurp>
                        <Cd>{{ctgy_purp}}</Cd>
                    </CtgyPurp>
                </PmtTpInf>
                <RmtInf>
                    <Ustrd>{{narration}}</Ustrd>
                </RmtInf>
                <UltmtDbtr>
                    <Nm>nusfgs</Nm>
                </UltmtDbtr>
                <Dbtr>
                    <Nm>{{sender_account_name}}</Nm>
                    <PstlAdr>
                        <Ctry>{{sender_country}}</Ctry>
                        <AdrLine>{{sender_address}}</AdrLine>
                    </PstlAdr>
                </Dbtr>
                <DbtrAcct>
                    <Id>
                        <Othr>
                            <Id>{{sender_account_number}}</Id>
                            <SchmeNm>
                                <Cd>BBAN</Cd>
                            </SchmeNm>
                        </Othr>
                    </Id>
                </DbtrAcct>
                <DbtrAgt>
                    <FinInstnId>
                        <BICFI>{{sender_bic_id}}</BICFI>
                        <ClrSysMmbId>
                            <MmbId>{{sender_papss_id}}</MmbId>
                        </ClrSysMmbId>
                    </FinInstnId>
                </DbtrAgt>
                <CdtrAgt>
                    <FinInstnId>
                        <BICFI>{{receiver_bic_id}}</BICFI>
                        <ClrSysMmbId>
                            <ClrSysId>
                                <Prtry>PAPSS</Prtry>
                            </ClrSysId>
                            <MmbId>{{receiver_papss_id}}</MmbId>
                        </ClrSysMmbId>
                    </FinInstnId>
                </CdtrAgt>
                <Cdtr>
                    <Nm>{{receiver_account_name}}</Nm>
                    <PstlAdr>
                        <Ctry>{{receiver_country}}</Ctry>
                        <AdrLine>{{receiver_address}}</AdrLine>
                    </PstlAdr>
                </Cdtr>
                <CdtrAcct>
                    <Id>
                        <Othr>
                            <Id>{{receiver_account_number}}</Id>
                            <SchmeNm>
                                <Cd>BBAN</Cd>
                            </SchmeNm>
                        </Othr>
                    </Id>
                </CdtrAcct>
                <UltmtCdtr>
                    <Nm>bsvmp</Nm>
                </UltmtCdtr>
            </OrgnlTxRef>
        </TxInf>
        <SplmtryData>
            <Envlp>
                <AuditInfo>
                    <ManualPaymentId>12584603</ManualPaymentId>
                    <IP>PAPSS-host</IP>
                    <PC>PAPSS-GUI</PC>
                </AuditInfo>
            </Envlp>
        </SplmtryData>
    </env:PmtRtr>
</env:Message>
