<hdr:Message xmlns:hdr="urn:montran:message.01">
    <hdr:AppHdr xmlns="urn:iso:std:iso:20022:tech:xsd:head.001.001.01">
        <Fr>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{sender_papss_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </Fr>
        <To>
            <FIId>
                <FinInstnId>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{toClearingSystemId}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </FIId>
        </To>
        <BizMsgIdr>{{messageId}}</BizMsgIdr>
        <MsgDefIdr>pacs.008.001.07</MsgDefIdr>
        <BizSvc>RTP</BizSvc>
        <CreDt>{{creationDateTime}}</CreDt>
    </hdr:AppHdr>
    <hdr:FIToFICstmrCdtTrf xmlns="urn:iso:std:iso:20022:tech:xsd:pacs.008.001.07">
        <GrpHdr>
            <MsgId>{{messageId}}</MsgId>
            <CreDtTm>{{creationDateTime}}</CreDtTm>
            <NbOfTxs>1</NbOfTxs>
            <TtlIntrBkSttlmAmt Ccy="{{sender_currency}}">{{sender_amount}}</TtlIntrBkSttlmAmt>
            <IntrBkSttlmDt>{{creationDateSimple}}</IntrBkSttlmDt>
            <SttlmInf>
                <SttlmMtd>CLRG</SttlmMtd>
                <ClrSys>
                    <Prtry>PAPSS</Prtry>
                </ClrSys>
            </SttlmInf>
            <PmtTpInf>
                <SvcLvl>
                    <Prtry>INST</Prtry>
                </SvcLvl>
                <LclInstrm>
                    <Cd>{{instrm_type}}</Cd>
                </LclInstrm>
                <CtgyPurp>
                    <Cd>{{ctgy_purp}}</Cd>
                </CtgyPurp>
            </PmtTpInf>
            <InstgAgt>
                <FinInstnId>
                    <BICFI>{{sender_bic_id}}</BICFI>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{sender_papss_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </InstgAgt>
        </GrpHdr>
        <CdtTrfTxInf>
            <PmtId>
                <InstrId>{{messageId}}</InstrId>
                <EndToEndId>{{messageId}}</EndToEndId>
                <TxId>{{messageId}}</TxId>
            </PmtId>
            <!--            <IntrBkSttlmAmt Ccy="{{receiver_currency}}">{{receiver_amount}}</IntrBkSttlmAmt> -->
            <IntrBkSttlmAmt Ccy="{{sender_currency}}">{{sender_amount}}</IntrBkSttlmAmt>
            <AccptncDtTm>{{creationDateTime}}</AccptncDtTm>
            <!--            <InstdAmt Ccy="{{sender_currency}}">{{sender_amount}}</InstdAmt>-->
            <InstdAmt Ccy="{{receiver_currency}}">{{receiver_amount}}</InstdAmt>
            <XchgRate>{{fx_rate}}</XchgRate>
            <ChrgBr>SLEV</ChrgBr>
            <UltmtDbtr>
                <Nm>{{sender_account_name}}</Nm>
            </UltmtDbtr>
            <Dbtr>
                <Nm>{{sender_account_name}}</Nm>
                <PstlAdr>
                    <Ctry>{{sender_country}}</Ctry>
                    <AdrLine>{{sender_address}}</AdrLine>
                </PstlAdr>
            </Dbtr>
            <DbtrAcct>
                <Id>
                    <Othr>
                        <Id>{{sender_account_number}}</Id>
                        <SchmeNm>
                            <Cd>BBAN</Cd>
                        </SchmeNm>
                    </Othr>
                </Id>
            </DbtrAcct>
            <DbtrAgt>
                <FinInstnId>
                    <BICFI>{{sender_bic_id}}</BICFI>
                    <ClrSysMmbId>
                        <MmbId>{{sender_papss_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </DbtrAgt>
            <CdtrAgt>
                <FinInstnId>
                    <BICFI>{{receiver_bic_id}}</BICFI>
                    <ClrSysMmbId>
                        <ClrSysId>
                            <Prtry>PAPSS</Prtry>
                        </ClrSysId>
                        <MmbId>{{receiver_papss_id}}</MmbId>
                    </ClrSysMmbId>
                </FinInstnId>
            </CdtrAgt>
            <Cdtr>
                <Nm>{{receiver_account_name}}</Nm>
                <PstlAdr>
                    <Ctry>{{receiver_country}}</Ctry>
                    <AdrLine>{{receiver_address}}</AdrLine>
                </PstlAdr>
            </Cdtr>
            <CdtrAcct>
                <Id>
                    <Othr>
                        <Id>{{receiver_account_number}}</Id>
                        <SchmeNm>
                            <Cd>BBAN</Cd>
                        </SchmeNm>
                    </Othr>
                </Id>
            </CdtrAcct>
            <UltmtCdtr>
                <Nm>bsvmp</Nm>
            </UltmtCdtr>
            <RmtInf>
                <Ustrd>{{narration}}</Ustrd>
            </RmtInf>
        </CdtTrfTxInf>
        <SplmtryData>
            <Envlp>
                <AuditInfo>
                    <IP>10.77.7.13</IP>
                    <PC>vi-papss</PC>
                </AuditInfo>
            </Envlp>
        </SplmtryData>
    </hdr:FIToFICstmrCdtTrf>
</hdr:Message>
